#include <iostream>
using namespace std;
int main() {
   int n1,m1,n2,m2;
   cin>>n1>>m1;
   int A[n1][m1];
   
   for(int i=0;i<n1;i++){
       for(int j=0;j<m1;j++){
           cin>>A[i][j];
       }
   }
   cin>>n2>>m2;
   int B[n2][m2];
   for(int i=0;i<n1;i++){
       for(int j=0;j<m1;j++){
           cin>>B[i][j];
       }
   }
  
   if(n1==n2 && m1==m2){
   for(int i=0;i<n1;i++){
       for(int j=0;j<m1;j++){
           cout<<B[i][j]+A[i][j]<<" ";
       }cout<<endl;
   }
       
   }
   
    return 0;
}
